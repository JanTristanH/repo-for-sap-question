{
    "donut": {
        "sap.app": {
            "id": "sample.CardsLayout.model.donut",
            "type": "card"
        },
        "sap.card": {
            "type": "Analytical",
            "header": {
                "type": "Numeric",
                "data": {
                    "json": {
                        "n": "123.45",
                        "u": "g",
                        "trend": "Up",
                        "valueColor": "Error"
                    }
                },
                "title": "",
                "mainIndicator": {
                    "number": "{n}",
                    "unit": "{u}",
                    "trend": "{trend}",
                    "state": "{valueColor}"
                },
                "sideIndicators": [{
                        "title": "Target",
                        "number": "100",
                        "unit": "g"
                    },
                    {
                        "title": "Deviation",
                        "number": "+23.45",
                        "unit": "%"
                    }
                ]
            },
            "content": {
                "chartType": "Donut",
                "legend": {
                    "visible": true,
                    "position": "Bottom",
                    "alignment": "Left"
                },
                "plotArea": {
                    "dataLabel": {
                        "visible": true,
                        "showTotal": true
                    }
                },
                "title": {
                    "visible": false
                },
                "measureAxis": "size",
                "dimensionAxis": "color",
                "data": {
                    "json": {
                        "measures": [{
                                "measureName": "Nitrous Oxide",
                                "value": 35
                            },
                            {
                                "measureName": "Methane",
                                "value": 40
                            },
                            {
                                "measureName": "Carbon Monoxide",
                                "value": 14
                            },
                            {
                                "measureName": "Carbon Dioxide",
                                "value": 8
                            },
                            {
                                "measureName": "Sulphur Dioxide",
                                "value": 7
                            },
                            {
                                "measureName": "Nitrogen Dioxide",
                                "value": 4
                            }
                        ]
                    },
                    "path": "/measures"
                },
                "dimensions": [{
                    "label": "Measure Name",
                    "value": "{measureName}"
                }],
                "measures": [{
                    "label": "Value",
                    "value": "{value}"
                }]
            }
        }
    },

    "objectVehicle": {
        "sap.app": {
            "type": "card"
        },
        "sap.card": {
            "type": "Object",
            "data": {
                "json": {
                    "type": "Combustion",
                    "emissionClass": "EURO 6c",
                    "avgConsumption": "35,7 L/100km",
                    "opfInstalled": "yes"
                }
            },
            "header": {
                "title": "Vehicle Details",
                "subTitle": "Vehicle ID"
            },
            "content": {
                "groups": [{
                        "title": "Transport Unit",
                        "items": [{
                                "label": "Type",
                                "value": "{type}"
                            },
                            {
                                "label": "Emission Class",
                                "value": "{emissionClass}"
                            },
                            {
                                "label": "Avg. Consumption",
                                "value": "{avgConsumption}"
                            },
                            {
                                "label": "OPF installed",
                                "value": "{opfInstalled}"
                            }
                        ]
                    },
                    {
                        "title": "Loaded Goods",
                        "items": [{
                                "label": "Diesel",
