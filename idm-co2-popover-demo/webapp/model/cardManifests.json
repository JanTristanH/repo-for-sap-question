{
    "donut": {
        "sap.app": {
            "id": "sample.CardsLayout.model.donut",
            "type": "card"
        },
        "sap.card": {
            "type": "Analytical",
            "header": {
                "type": "Numeric",
                "data": {
                    "json": {
                        "n": "123.45",
                        "u": "g",
                        "trend": "Up",
                        "valueColor": "Error"
                    }
                },
                "title": "Emisson by pollutant in {u}",
                "mainIndicator": {
                    "number": "{n}",
                    "unit": "{u}",
                    "trend": "{trend}",
                    "state": "{valueColor}"
                },
                "sideIndicators": [{
                        "title": "Target",
                        "number": "100",
                        "unit": "g"
                    },
                    {
                        "title": "Deviation",
                        "number": "+23.45",
                        "unit": "%"
                    }
                ]
            },
            "content": {
                "chartType": "Donut",
                "legend": {
                    "visible": true,
                    "position": "Side",
                    "alignment": "Right"
                },
                "plotArea": {
                    "dataLabel": {
                        "visible": true,
                        "showTotal": true
                    }
                },
                "title": {
                    "visible": false
                },
                "measureAxis": "size",
                "dimensionAxis": "color",
                "data": {
                    "json": {
                        "measures": [{
                                "measureName": "Nitrous Oxide",
                                "value": 35
                            },
                            {
                                "measureName": "Methane",
                                "value": 40
                            },
                            {
                                "measureName": "Carbon Monoxide",
                                "value": 14
                            },
                            {
                                "measureName": "Carbon Dioxide",
                                "value": 8
                            },
                            {
                                "measureName": "Sulphur Dioxide",
                                "value": 7
                            },
                            {
                                "measureName": "Nitrogen Dioxide",
                                "value": 4
                            }
                        ]
                    },
                    "path": "/measures"
                },
                "dimensions": [{
                    "label": "Measure Name",
                    "value": "{measureName}"
                }],
                "measures": [{
                    "label": "Value",
                    "value": "{value}"
                }]
            }
        }
    },

    "objectVehicle": {
        "sap.app": {
            "type": "card"
        },
        "sap.card": {
            "type": "Object",
            "header": {
                "title": "Vehicle Details",
                "subTitle": "Vehicle_ID"
            },
            "content": {
                "groups": [{
                        "title": "Transport Unit",
                        "items": [{
                                "label": "Type",
                                "value": "{co2popover>/type}"
                            },
                            {
                                "label": "Emission Class",
                                "value": "{co2popover>/emissionClass}"
                            },
                            {
                                "label": "Avg. Consumption",
                                "value": "{co2popover>/avgConsumption}"
                            },
                            {
                                "label": "OPF installed",
                                "value": "{co2popover>/opfInstalled}"
                            }
                        ]
                    },
                    {
                        "title": "Loaded Goods",
                        "items": [{
                                "label": "Diesel",
                                "value": "1500 L"
                            },
                            {
                                "label": "Petrol",
                                "value": "2500 L"
                            }
                        ]
                    }
                ]
            }
        }
    },

    "objectRoute": {
        "sap.app": {
            "type": "card"
        },
        "sap.card": {
            "type": "Object",
            "content": {
                "groups": [{
                    "title": "Route",
                    "items": [{
                            "label": "Distance",
                            "value": "{co2popover>/distance} {co2popover>/distanceUoM}"
                        },
                        {
                            "label": "Duration",
                            "value": "{co2popover>/durationHours}h {co2popover>/durationMinutes}m"
                        },
                        {
                            "label": "Fuel Consumption",
                            "value": "{co2popover>/fuelConsumption} {co2popover>/fuelConsumptionUoM}"
                        }
                    ]
                }]
            }
        }
    },

    "tableCalculatedEmissions": {
        "sap.app": {
            "id": "sample.CardsLayout.model.table",
            "type": "card"
        },
        "sap.card": {
            "type": "Table",
            "header": {
                "title": "Calculated Emission",
                "subTitle": "",
                "status": {
                    "text": "5 of 5"
                }
            },
            "content": {
                "data": {
                    "path": "co2popover>/emissionDetails"
                },
                "row": {
                    "columns": [{
                            "title": "Key",
                            "value": "{co2popover>key}",
                            "identifier": true
                        },
                        {
                            "title": "Description",
                            "value": "{co2popover>description}"
                        },
                        {
                            "title": "Amount",
                            "value": "{co2popover>amount} {co2popover>amountUom}"
                        },
                        {
                            "title": "CO2e Factor",
                            "value": "{co2popover>co2eFactor}",
                            "state": "{co2popover>co2eFactorState}"
                        },
                        {
                            "title": "CO2e Amount",
                            "value": "{co2popover>co2eAmount} {co2popover>co2eAmountUom}"
                        }
                    ]
                }
            }
        }
    }

}