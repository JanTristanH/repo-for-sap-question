{
    "donut": {
        "sap.app": {
            "id": "sample.CardsLayout.model.donut",
            "type": "card"
        },
        "sap.card": {
            "type": "Analytical",
            "header": {
                "type": "Numeric",
                "title": "Emisson by pollutant in {co2popover>/emissionDetailsIndicators/unit}",
                "mainIndicator": {
                    "number": "{co2popover>/emissionDetailsIndicators/value}",
                    "unit": "{co2popover>/emissionDetailsIndicators/unit}",
                    "trend": "{co2popover>/emissionDetailsIndicators/trend}",
                    "state": "{co2popover>/emissionDetailsIndicators/valueColor}"
                },
                "sideIndicators": [{
                        "title": "Target",
                        "number": "{co2popover>/emissionDetailsIndicators/target/number}",
                        "unit": "{co2popover>/emissionDetailsIndicators/target/unit}"
                    },
                    {
                        "title": "Deviation",
                        "number": "{co2popover>/emissionDetailsIndicators/deviation/number}",
                        "unit": "{co2popover>/emissionDetailsIndicators/deviation/unit}"
                    }
                ]
            },
            "content": {
                "chartType": "Donut",
                "legend": {
                    "visible": true,
                    "position": "Right",
                    "alignment": "Center"
                },
                "plotArea": {
                    "dataLabel": {
                        "visible": true,
                        "showTotal": true
                    }
                },
                "title": {
                    "visible": false
                },
                "measureAxis": "size",
                "dimensionAxis": "color",
                "data": {
                    "json": {
                        "emissionDetails": [{
                                "key": "NO2",
                                "description": "Nitrous oxide",
                                "amount": "106.17",
                                "co2eFactor": "298",
                                "co2eFactorState": "Error",
                                "co2eAmount": "31,638.66",
                                "amountUom": "g",
                                "co2eAmountUom": "g"
                            },
                            {
                                "key": "CH4",
                                "description": "Methane",
                                "amount": "0.057",
                                "co2eFactor": "25",
                                "co2eFactorState": "Warning",
                                "co2eAmount": "1.42",
                                "amountUom": "g",
                                "co2eAmountUom": "g"
                            },
                            {
                                "key": "CO",
                                "description": "Carbon Monoxide",
                                "amount": "15.85",
                                "co2eFactor": "",
                                "co2eFactorState": "None",
                                "co2eAmount": "",
                                "amountUom": "g",
                                "co2eAmountUom": ""
                            },
                            {
                                "key": "CO2",
                                "description": "Carbon Dioxide",
                                "amount": "21.73",
                                "co2eFactor": "1",
                                "co2eFactorState": "None",
                                "co2eAmount": "21.73",
                                "amountUom": "g",
                                "co2eAmountUom": "g"
                            },
                            {
                                "key": "SO2",
                                "description": "Sulphur Dioxide",
                                "amount": "0.11",
                                "co2eFactor": "",
                                "co2eFactorState": "None",
                                "co2eAmount": "",
                                "amountUom": "",
                                "co2eAmountUom": ""
                            }

                        ]
                    },
                    "path": "/emissionDetails"
                },
                "dimensions": [{
                    "label": "Measure Name",
                    "value": "{description}"
                }],
                "measures": [{
                    "label": "Value",
                    "value": "{co2eAmount}"
                }]
            }
        }
    },

    "objectVehicle": {
        "sap.app": {
            "id": "sample.CardsLayout.model.objectVehicle",
            "type": "card"
        },
        "sap.card": {
            "type": "Object",
            "header": {
                "title": "Vehicle Details",
                "subTitle": "Vehicle_ID"
            },
            "content": {
                "groups": [{
                    "title": "Transport Unit",
                    "items": [{
                            "label": "Type",
                            "value": "{co2popover>/type}"
                        },
                        {
                            "label": "Emission Class",
                            "value": "{co2popover>/emissionClass}"
                        },
                        {
                            "label": "Avg. Consumption",
                            "value": "{co2popover>/avgConsumption}"
                        },
                        {
                            "label": "OPF installed",
                            "value": "{co2popover>/opfInstalled}"
                        }
                    ]
                }]
            }
        }
    },

    "objectRoute": {
        "sap.app": {
            "id": "sample.CardsLayout.model.objectRoute",
            "type": "card"
        },
        "sap.card": {
            "type": "Object",
            "content": {
                "groups": [{
                    "title": "Route",
                    "items": [{
                            "label": "Distance",
                            "value": "{co2popover>/distance} {co2popover>/distanceUoM}"
                        },
                        {
                            "label": "Duration",
                            "value": "{co2popover>/durationHours}h {co2popover>/durationMinutes}m"
                        },
                        {
                            "label": "Fuel Consumption",
                            "value": "{co2popover>/fuelConsumption} {co2popover>/fuelConsumptionUoM}"
                        }
                    ]
                }]
            }
        }
    },

    "tableCalculatedEmissions": {
        "sap.app": {
            "id": "sample.CardsLayout.model.tableCalculatedEmissions",
            "type": "card"
        },
        "sap.card": {
            "type": "Table",
            "header": {
                "title": "Calculated Emission",
                "subTitle": "",
                "status": {
                    "text": "5 of 5"
                }
            },
            "content": {
                "data": {
                    "path": "co2popover>/emissionDetails"
                },
                "row": {
                    "columns": [{
                            "title": "Key",
                            "value": "{co2popover>key}",
                            "identifier": true
                        },
                        {
                            "title": "Description",
                            "value": "{co2popover>description}"
                        },
                        {
                            "title": "Amount",
                            "value": "{co2popover>amount} {co2popover>amountUom}"
                        },
                        {
                            "title": "CO2e Factor",
                            "value": "{co2popover>co2eFactor}",
                            "state": "{co2popover>co2eFactorState}"
                        },
                        {
                            "title": "CO2e Amount",
                            "value": "{co2popover>co2eAmount} {co2popover>co2eAmountUom}"
                        }
                    ]
                }
            }
        }
    },

    "listLoadedGoods": {
        "sap.app": {
            "id": "sample.CardsLayout.model.listLoadedGoods",
            "type": "card"
        },
        "sap.card": {
            "type": "List",
            "header": {
                "title": "Loaded Goods",
                "icon": {
                    "src": "sap-icon://product"
                }
            },
            "content": {
                "data": {
                    "path": "co2popover>/loadedGoods"
                },
                "maxItems": "3",
                "item": {
                    "title": "{co2popover>name}",
                    "description": "{co2popover>quantity} {co2popover>uom}",
                    "highlight": "None"
                }
            }
        }
    }

}